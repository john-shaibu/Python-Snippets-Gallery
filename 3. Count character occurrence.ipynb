    "def count_character(character):\n",
    "    count = {}\n",
    "    for i in character:\n",
    "        if i in count:\n",
    "            count[i] += 1\n",
    "        else:\n",
    "            count[i] = 1\n",
    "    print(count)\n",
    "\n",
    "word = input('Enter String: ')\n",
    "print(count_character(word))"
   ]
